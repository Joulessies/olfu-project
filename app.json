{
  "expo": {
    "name": "OLFU Commute Smart",
    "slug": "olfu-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/olfu-logo.png",
    "scheme": "olfuproject",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.olfu.commutesmart",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "OLFU Commute Smart needs your location to show your position on the map and provide navigation assistance.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "OLFU Commute Smart needs your location to enable safety features like geofencing and friend tracking.",
        "NSLocationAlwaysUsageDescription": "OLFU Commute Smart needs background location access to keep you safe with geofence alerts.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2E7D32",
        "foregroundImage": "./assets/images/olfu-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.olfu.commutesmart",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "VIBRATE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/olfu-logo.png",
      "bundler": "metro",
      "name": "OLFU Commute Smart",
      "shortName": "OLFU Commute",
      "description": "Smart commuting app for OLFU students with safety features, navigation, and friend tracking",
      "themeColor": "#2E7D32",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow OLFU Commute Smart to use your location to provide safety features and navigation."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/olfu-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2E7D32",
          "dark": {
            "backgroundColor": "#1B5E20"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b1097335-48f0-4e55-91ce-f108d6b7a844"
      }
    },
    "owner": "joulessies"
  }
}
